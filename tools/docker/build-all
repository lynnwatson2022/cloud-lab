#!/bin/sh

TOP_DIR=$(cd $(dirname $0)/../../ && pwd)
source $TOP_DIR/tools/docker/config $* >/dev/null

# markdown-lab must be built before qing-lab

for i in `ls $CONFIGS_DIR | tr -s ' '`
do
    echo -e "\n\n========== Building tinylab/$i ==========\n\n"

    ${TOP_DIR}/tools/docker/build $i

    [ $? -ne 0 ] && exit 1
done
